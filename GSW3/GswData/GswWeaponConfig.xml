<?xml version="1.0" encoding="utf-8"?>
<GswWeaponConfig.xml>
    <!-- Here you can set WeaponList that will used by GSW3.
    If any weapon was not added, GSW3 will can't detect which weapon've hit ped and message about that to Console. -->
    <WeaponList>
        <Weapon>
            <!-- LocalizationKey needed for Localization in future, 
            currently this key will used in wound notifications and console -->
            <LocalizationKey Key="SMALL_CALIBER_WEAPON"/>
            <!-- In hashes you need to add hashes of weapons in this WeaponGroup -->
            <!-- Pistol;PistolMk2;CombatPistol;SNSPistol;SNSPistolMk2;VintagePistol;MarksmanPistol;DoubleActionRevolver;APPistol;MicroSMG;MiniSMG;SMG;SMGMk2;CombatPDW;MachinePistol; -->
            <Hashes Hashes="453432689;3219281620;1593441988;3218215474;2285322324;137902532;3696079510;2548703416;584646201;324215364;3173288789;736523883;2024373456;171789620;3675956304;"/>
            <!-- WeaponArmorStats will be used for penetration checks.
            If CanPenetrateArmor is FALSE, weapon will NEVER penetrate armor.
            ChanceToPenetrateHelmet will used for Helmet Penetration Check.
            ArmorDamage will set how many points of armor will be removed, when this weapon hit armor.
            MinArmorPercentForPenetration="0.3" mean when armor percent is more 0.3, you never penetrate armor,
            when armor percent 0.25 chance to penetrate will be 1 - 0.25/0.3 ~ 1 - 0.83 ~ 17% -->
            <WeaponArmorStats CanPenetrateArmor="TRUE" ChanceToPenetrateHelmet="0.2" ArmorDamage="3" MinArmorPercentForPenetration="0.3"/>
            <DamageMult Value="1.0"/>
            <PainMult Value="1.0"/>
            <BleedingMult Value="1.0"/>
            <CritChance Value="0.4"/>
            <WeaponWoundList>
                <WeaponWound Uid="GRAZE_GSW" Weight="1"/>
                <WeaponWound Uid="FLESH_GSW" Weight="3"/>
                <WeaponWound Uid="PENETRATING_GSW" Weight="7"/>
                <WeaponWound Uid="PERFORATING_GSW" Weight="4"/>
                <WeaponWound Uid="AVULSIVE_GSW" Weight="1"/>
            </WeaponWoundList>
        </Weapon>
        <Weapon>
